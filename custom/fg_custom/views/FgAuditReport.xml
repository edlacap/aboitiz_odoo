<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_fg_audit_report">
         Run Report Date,<t t-esc="run_report_date"/>
         Generated By,<t t-esc="generated_by"/>
         Company,<t t-esc="name"/>
         VAT Reg TIN,<t t-esc="tin"/>
         Address,<t t-esc="address"/>

        Name, <t t-esc="auditLogs"/>
    </template>

    <record id="fg_audit_report" model="ir.actions.report">
        <field name="name">Audit Log Report</field>
        <field name="model">pos.session</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">fg_custom.report_fg_audit_report</field>
        <field name="report_file">fg_custom.report_fg_audit_report</field>
        <field name="print_report_name">'FG Audit Log Report'</field>
    </record>

</odoo>