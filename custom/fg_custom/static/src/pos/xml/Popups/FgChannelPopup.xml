<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="FgChannelPopup" owl="1">
        <div role="dialog" class="modal-dialog">
            <Draggable>
                <div class="popup drag-handle">
                    <main class="body">
                        <div class="fg-custom-pos-payment">
                            <div class="card_number">
                                <span class="input-type">
                                    Channel
                                </span>
                                <div class="input-field">
                                    <select class="detail" name="x_ext_source" t-model="state.x_ext_source">
                                        <option value = "Food Service">Food Service</option>
                                        <option value = "Lazada">Lazada</option>
                                        <option value = "Reseller">Reseller</option>
                                        <option value = "Retail">Retail</option>
                                        <option value = "Shopee">Shopee</option>
                                        <option value = "Traders">Traders</option>
                                        <option value = "Website">Website</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </main>
                    <footer class="footer">
                        <div class="button confirm disable" t-on-click="confirm">
                            Confirm
                        </div>
                        <div class="button cancel" t-on-click="cancel">
                            <t t-esc="props.cancelText" />
                        </div>
                    </footer>
                </div>
            </Draggable>
        </div>
    </t>

</templates>